{
 "name": "pg-connector",
 "config": {
   "max.batch.size": 100,
   "producer.override.compression.type": "lz4",
   "producer.override.linger.ms": "3000",
   "connector.class": "io.debezium.connector.postgresql.PostgresConnector",
   "database.hostname": "postgres",
   "database.port": "5432",
   "database.user": "postgres-user",
   "database.password": "postgres-pw",
   "database.dbname": "online",
   "table.whitelist": "online.users,online.orders",
   "transforms": "unwrap",
   "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
   "transforms.unwrap.drop.tombstones": "false",
   "transforms.unwrap.delete.handling.mode": "rewrite",
   "topic.prefix": "online",
   "topic.creation.enable": "true",
   "topic.creation.default.replication.factor": "-1",
   "topic.creation.default.partitions": "-1",
   "skipped.operations": "none",
   "snapshot.mode": "initial",
   "signal.kafka.topic": "incr_signal",
   "signal.kafka.bootstrap.servers": "kafka-0:9092,kafka-1:9092,kafka-2:9092",
   "heartbeat.interval.ms": 1000,
   "topic.heartbeat.prefix": "heartbeat"
 }
}
